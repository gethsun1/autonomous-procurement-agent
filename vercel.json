{
    "version": 2,
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    },
    "functions": {
        "api/index.ts": {
            "memory": 1024,
            "maxDuration": 30
        }
    }
}